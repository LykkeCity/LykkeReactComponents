{
  "name": "lykke-react-components",
  "version": "0.1.11",
  "description": "React Components for Lykke projects",
  "keywords": [
    "Lykke"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreyzhylin/LykkeReactComponents.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest",
    "build-css": "node-sass-chokidar src/ -o src/ && node-sass-chokidar src/ -o dist/",
    "prebuild": "rimraf dist & yarn build-css",
    "build": "yarn test && tsc -p . && rollup -c",
    "storybook": "yarn build && start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "main": "./dist/bundle/lykke-react-components.js",
  "module": "./dist/index.js",
  "jsnext:main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-info": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.0",
    "@types/node": "^8.0.31",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/storybook__addon-actions": "^3.0.3",
    "@types/storybook__addon-info": "^3.2.2",
    "@types/storybook__addon-links": "^3.3.0",
    "@types/storybook__react": "^3.0.7",
    "LykkeFramework": "^0.3.40",
    "babel-core": "^6.26.0",
    "bootstrap": "3",
    "classnames": "^2.2.5",
    "compass-mixins": "0.12.10",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "lint-staged": "^4.2.3",
    "node-sass-chokidar": "^1.1.0",
    "prettier": "^1.7.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.5",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-css-porter": "^0.1.2",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^3.0.0",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.1",
    "ts-loader": "^3.5.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.5.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "uglify-es": "^3.3.9"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,scss,sass}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": "tslint --fix",
    "stories/**/*.{ts,tsx,js,jsx,json,css,scss,sass}": [
      "prettier --write",
      "git add"
    ],
    "stories/**/*.{ts,tsx}": "tslint --fix"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/src/__tests__/.*)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {}
}
